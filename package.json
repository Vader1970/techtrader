{
  "name": "techtrader",
  "version": "1.0.0",
  "description": "The backend for techlines",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "client": "npm run start --prefix client",
    "server": "node server/index.js",
    "app": "concurrently npm \"npm run server\" \"npm run client\"",
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install",
    "install-server": "cd server && npm install",
    "postinstall": "npm run install-client && npm run install-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vader1970/techtrader.git"
  },
  "author": "Dan Wilkey Supriya Sharma Tin Mariano",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Vader1970/techtrader/issues"
  },
  "homepage": "https://github.com/Vader1970/techtrader#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.3",
    "concurrently": "^7.6.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^6.9.0",
    "multer": "^2.0.2",
    "multer-storage-cloudinary": "^4.0.0"
  }
}